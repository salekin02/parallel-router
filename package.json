{
  "name": "parallel-router",
  "version": "0.0.10",
  "description": "Enhanced React Router v6 with parallel routing - display multiple routes simultaneously with elegant sidebar navigation",
  "main": "./dist/index.js",
  "homepage": "https://github.com/salekin02/parallel-router#readme",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-v6",
    "router",
    "parallel-routes",
    "parallel-routing",
    "sidebar",
    "navigation",
    "drawer",
    "modal-routing",
    "multi-pane",
    "enhanced-router"
  ],
  "license": "GPL-3.0",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salekin02/parallel-router.git"
  }
}
